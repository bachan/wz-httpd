<wzconfig>
    <pid_file_name>@CMAKE_INSTALL_PREFIX@/var/run/wz.pid</pid_file_name>
    <log_file_name>@CMAKE_INSTALL_PREFIX@/var/log/wz.log</log_file_name>
    <log_level>notice</log_level>

    <access_log_file_name>@CMAKE_INSTALL_PREFIX@/var/log/wz.access.log</access_log_file_name>

    <listen port="8088" backlog="1024" />

    <plugin name = "status"
            path = "@CMAKE_INSTALL_PREFIX@/lib64/libwz_status.so"
            opts = "@CMAKE_INSTALL_PREFIX@/etc/wz_status.plugin.xml" />
</wzconfig>
